{
  "version": 2,
  "builds": [
    {
      "src": "api/transcript.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/transcript", "dest": "/api/transcript.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}